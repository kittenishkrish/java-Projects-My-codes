package hello;

import java.util.Scanner;

public class Basiccal {
	public static void main(String[] args) {
		Scanner krish =new Scanner(System.in);
		double firstvalue = krish.nextDouble();
		double Secondvalue = krish.nextDouble();	
		double answer = firstvalue + Secondvalue;
		System.out.println("sum is " + answer);
	}
}
                                                                                                      
